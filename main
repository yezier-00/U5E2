import java.util.List;
import java.util.ArrayList;
import java.util.Scanner;
public class main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//metodo quicksort con una lista
		Scanner sn=new Scanner(System.in);
		int vector[]=new int[5];
		List <> lista=new ArrayList<>();
		for (int i = 0; i < 5; i++) {
			System.out.println("dame elementos de la lista:");
			int elemento=sn.nextInt();
			lista.add(elemento);
			//vector[i]=sn.nextInt();
		}
		System.out.println("lista desordenada:");
		for (int i = 0; i < vector.length; i++) {
			System.out.println(vector[i]);
		}
		//System.out.println(lista);
		System.out.println("lista ordenada:");
		main obj=new main();
		obj.quick();
		
	}//main
public void quick() {
			int i,j,pivote,aux,primero,ultimo;
			i=primero;
			j=ultimo;
			pivote=lista[primero+ultimo]/2;
			do {
				while (lista[i]<pivote) {
					i++;
				}
				while (lista[j]>pivote) {
					j--;
				}
				if (i<=j) {
					aux=lista[i];
					lista[i]=lista[j];
					lista[j]=aux;
					i++;
					j--;
				}
			} while (i<=j);
			
		}
}//class
